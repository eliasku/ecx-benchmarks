package ecx.benchmarks;

class EcxPosition extends Component {
    public var x:Float = 0;
    public var y:Float = 0;

    public function new() {}

    public function randomize():EcxPosition {
        x = Math.random();
        y = Math.random();
        return this;
    }
}

