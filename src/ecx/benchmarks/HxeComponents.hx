package ecx.benchmarks;

import hxE.Component;

class HxePosition extends Component {
    public var x:Float = 0;
    public var y:Float = 0;

    public function new() {
        super();
    }

    public function randomize():HxePosition {
        x = Math.random();
        y = Math.random();
        return this;
    }
}

